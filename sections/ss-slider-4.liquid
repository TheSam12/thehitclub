<link
  rel="stylesheet"
  href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css"
/>

{%- liquid 
  assign padding_horizontal = section.settings.padding_horizontal
  assign padding_horizontal_mobile = section.settings.padding_horizontal_mobile
  assign padding_top = section.settings.padding_top
  assign padding_bottom = section.settings.padding_bottom
  assign border_color = section.settings.border_color
  assign border_thickness = section.settings.border_thickness
  assign margin_top = section.settings.margin_top
  assign margin_bottom = section.settings.margin_bottom
  assign margin_horizontal_mobile = section.settings.margin_horizontal_mobile
  assign margin_horizontal = section.settings.margin_horizontal
  assign background_color = section.settings.background_color
  assign background_gradient = section.settings.background_gradient
  assign full_width = section.settings.full_width
  assign content_width = section.settings.content_width
  assign lazy = section.settings.lazy
  assign section_radius = section.settings.section_radius

  assign heading = section.settings.heading
  assign heading_size = section.settings.heading_size
  assign heading_size_mobile = section.settings.heading_size_mobile
  assign heading_color = section.settings.heading_color
  assign heading_custom = section.settings.heading_custom
  assign heading_font = section.settings.heading_font
  assign heading_height = section.settings.heading_height
  assign heading_align = section.settings.heading_align
  assign heading_align_mobile = section.settings.heading_align_mobile

  assign slider_view = section.settings.slider_view
  assign slider_view_mobile = section.settings.slider_view_mobile
  assign slider_gap = section.settings.slider_gap
  assign slider_gap_mobile = section.settings.slider_gap_mobile
  assign slider_mt = section.settings.slider_mt
  assign slider_mt_mobile = section.settings.slider_mt_mobile

  assign image_ratio_mobile = section.settings.image_ratio_mobile
  assign image_ratio = section.settings.image_ratio
  assign image_shape = section.settings.image_shape
  assign image_border_color = section.settings.image_border_color
  assign image_border_thickness = section.settings.image_border_thickness
  assign image_top_left_radius = section.settings.image_top_left_radius
  assign image_top_right_radius = section.settings.image_top_right_radius
  assign image_bottom_left_radius = section.settings.image_bottom_left_radius
  assign image_bottom_right_radius = section.settings.image_bottom_right_radius

  assign title_size = section.settings.title_size
  assign title_size_mobile = section.settings.title_size_mobile
  assign title_color = section.settings.title_color
  assign title_custom = section.settings.title_custom
  assign title_font = section.settings.title_font
  assign title_height = section.settings.title_height
  assign title_align = section.settings.title_align
  assign title_align_mobile = section.settings.title_align_mobile
  assign title_mt = section.settings.title_mt

  assign navigation_align_mobile = section.settings.navigation_align_mobile
  assign navigation_align = section.settings.navigation_align
  assign navigation_mt = section.settings.navigation_mt
  assign navigation_gap = section.settings.navigation_gap

  assign arrow_left = section.settings.arrow_left
  assign arrow_right = section.settings.arrow_right
  assign arrow_position = section.settings.arrow_position
  assign arrow_size = section.settings.arrow_size
  assign arrow_size_mobile = section.settings.arrow_size_mobile
  assign arrow_icon_size = section.settings.arrow_icon_size
  assign arrow_icon_size_mobile = section.settings.arrow_icon_size_mobile
  assign arrow_color = section.settings.arrow_color
  assign arrow_hover_color = section.settings.arrow_hover_color
  assign arrow_bg_color = section.settings.arrow_bg_color
  assign arrow_bg_hover_color = section.settings.arrow_bg_hover_color
  assign arrow_border_thickness = section.settings.arrow_border_thickness
  assign arrow_border_color = section.settings.arrow_border_color
  assign arrow_border_hover_color = section.settings.arrow_border_hover_color
  assign arrow_show_desktop = section.settings.arrow_show_desktop
  assign arrow_show_mobile = section.settings.arrow_show_mobile

  assign dots_size = section.settings.dots_size
  assign dots_size_mobile = section.settings.dots_size_mobile
  assign dots_show_desktop = section.settings.dots_show_desktop
  assign dots_show_mobile = section.settings.dots_show_mobile
  assign dots_color = section.settings.dots_color
-%}

{%- style -%}

  {{  heading_font | font_face: font_display: 'swap' }}
  {{  title_font | font_face: font_display: 'swap' }}
  
  .section-{{ section.id }} {
    border-top: solid {{ border_color }} {{ border_thickness }}px;
    border-bottom: solid {{ border_color }} {{ border_thickness }}px;
    padding-top: {{ padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ padding_bottom | times: 0.75 | round: 0 }}px;
    margin-top: {{ margin_top | times: 0.75 | round: 0 }}px;
    margin-bottom: {{ margin_bottom | times: 0.75 | round: 0 }}px;
    margin-left: {{ margin_horizontal_mobile }}rem;
    margin-right: {{ margin_horizontal_mobile }}rem;
    border-radius: {{ section_radius | times: 0.6 | round: 0 }}px;
    overflow: hidden;
  }
  
  .section-{{ section.id }}-settings {
    margin: 0 auto;
    padding-left: {{ padding_horizontal_mobile }}rem;
    padding-right: {{ padding_horizontal_mobile }}rem;
  }

  .slider-top-{{ section.id }} {
    display: flex;
    align-items: center;
    justify-content: space-between;
    gap: 16px;
  }

  .slider-buttons-{{ section.id }} {
    align-items: center;
    justify-content: space-between;
    gap: {{ navigation_gap | times: 0.75 | round: 0 }}px;
  }

  .slider-heading-{{ section.id }} {
    width: 100%;
  }

  .slider-heading-{{ section.id }} * {
    margin: 0;
    font-size: {{ heading_size_mobile }}px;
    color: {{ heading_color }};
    line-height: {{ heading_height }}%;
    text-transform: unset;
  }

  .slider-slider-{{ section.id }} {
    padding-left: {{ padding_horizontal_mobile }}rem;
    padding-right: {{ padding_horizontal_mobile }}rem;
    margin-top: {{ slider_mt_mobile }}px;
  }

  .slider-slide-{{ section.id }} {
    text-decoration: none;
  }

  .slider-image-{{ section.id }} {
    width: 100%;
    overflow: hidden;
  }

  .slider-image-{{ section.id }} img,
  .slider-image-{{ section.id }} svg {
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .slider-image-{{ section.id }} svg {
    background-color: #AFAFAF;
  }

  .clip-{{ section.id }} {
    width: 100%;
    height: 100%;
    pointer-events: none;
    position: absolute;
    top: 0%;
    bottom: 0%;
    left: 0%;
    right: 0%;
    background-color: transparent !important;
  }

  .slider-title-{{ section.id }} {
    text-align: {{ title_align_mobile }};
    margin-top: {{ title_mt | times: 0.75 | round: 0 }}px;
  }

  .slider-title-{{ section.id }} * {
    margin: 0;
    font-size: {{ title_size_mobile }}px;
    color: {{ title_color }};
    line-height: {{ title_height }}%;
    text-transform: unset;
  }

  .slider-navigation-{{ section.id }} {
    display: flex;
    align-items: center;
    gap: {{ navigation_gap | times: 0.75 | round: 0 }}px;
    margin-top: {{ navigation_mt | times: 0.75 | round: 0 }}px;
    justify-content: {{ navigation_align_mobile }};
  }

  .slider-btn-prev-{{ section.id }},
  .slider-btn-next-{{ section.id }} {
    position: static !important;
    width: {{ arrow_size_mobile }}px;
    flex-shrink: 0;
    height: {{ arrow_size_mobile }}px;
    border: {{ arrow_border_thickness }}px solid {{ arrow_border_color }};
    border-radius: 50%;
    background: transparent;
    background: {{ arrow_bg_color | hex_to_rgba: 0.8 }};
    align-items: center;
    justify-content: center;
    transition: all 0.3s ease 0s;
    cursor: pointer;
    -webkit-appearance: none;
    padding: 0px;
  }

  .slider-btn-prev-{{ section.id }} svg,
  .slider-btn-next-{{ section.id }} svg,
  .slider-btn-prev-{{ section.id }} img,
  .slider-btn-next-{{ section.id }} img {
    height: {{ arrow_icon_size_mobile }}px;
    flex: 0 0 {{ arrow_icon_size_mobile }}px;
  }

  .slider-btn-next-{{ section.id }} svg {
    transform: rotate(180deg);
    margin-left: 2px;
  }

  .slider-btn-prev-{{ section.id }} svg {
    margin-left: -2px;
  }

  .slider-btn-prev-{{ section.id }} svg path,
  .slider-btn-next-{{ section.id }} svg path {
    stroke: {{ arrow_color }};
    transition: all 0.3s ease 0s;
  }

  .slider-btn-prev-{{ section.id }}:hover ,
  .slider-btn-next-{{ section.id }}:hover {
    transition: all 0.3s ease 0s;
    border: {{ arrow_border_thickness }}px solid {{ arrow_border_hover_color }};
    background: {{ arrow_bg_hover_color }};
  }

  .slider-btn-prev-{{ section.id }}:hover svg path,
  .slider-btn-next-{{ section.id }}:hover svg path {
    stroke: {{ arrow_hover_color }};
    transition: all 0.3s ease 0s;
  }

  .slider-btn-prev-{{ section.id }}.swiper-button-disabled,
  .slider-btn-next-{{ section.id }}.swiper-button-disabled {
    opacity: 0.3;
  }

  .slider-pagination-{{ section.id }} {
    position: static !important;
    width: 100%;
    max-width: fit-content;
    display: flex;
    gap: 5px;
    justify-content: center;
    align-items: center;
  }

  .slider-pagination-{{ section.id }} .swiper-pagination-bullet {
    flex: 0 0 {{ dots_size_mobile }}px;
    width: {{ dots_size_mobile }}px;
    height: {{ dots_size_mobile }}px;
    background-color: transparent !important;
    transition: all 0.25s ease 0s;
    border: 1px solid {{ dots_color }} !important;
    opacity: 1 !important;
  }

  .slider-pagination-{{ section.id }} .swiper-pagination-bullet.swiper-pagination-bullet-active {
    background-color: {{ dots_color }} !important;
    transition: all 0.25s ease 0s;
  }
  
  @media(min-width: 1024px) {

    .section-{{ section.id }} {
      margin-top: {{ margin_top }}px;
      margin-bottom: {{ margin_bottom }}px;
      margin-left: {{ margin_horizontal }}rem;
      margin-right: {{ margin_horizontal }}rem;
      border-radius: {{ section_radius }}px;
      padding-top: {{ padding_top }}px;
      padding-bottom: {{ padding_bottom }}px;
    }
    
    .section-{{ section.id }}-settings {
      padding: 0 5rem;
      padding-left: {{ padding_horizontal }}rem;
      padding-right: {{ padding_horizontal }}rem;
    }

    .slider-heading-{{ section.id }} * {
      font-size: {{ heading_size }}px;
    }

    .slider-slider-{{ section.id }} {
        padding-left: {{ padding_horizontal }}rem;
        padding-right: {{ padding_horizontal }}rem;
        margin-top: {{ slider_mt }}px;
    }

    .slider-buttons-{{ section.id }} {
        gap: {{ navigation_gap }}px;
    }

    .slider-title-{{ section.id }} {
        text-align: {{ title_align }};
        margin-top: {{ title_mt }}px;
    }

    .slider-title-{{ section.id }} * {
        font-size: {{ title_size }}px;
    }

    .slider-navigation-{{ section.id }} {
      margin-top: {{ navigation_mt }}px;
      justify-content: {{ navigation_align }};
      gap: {{ navigation_gap }}px;
    }

    .slider-btn-prev-{{ section.id }},
    .slider-btn-next-{{ section.id }} {
      width: {{ arrow_size }}px;
      height: {{ arrow_size }}px;
    }

    .slider-btn-prev-{{ section.id }} svg,
    .slider-btn-next-{{ section.id }} svg {
      height: {{ arrow_icon_size }}px;
      flex: 0 0 {{ arrow_icon_size }}px;
    }

    .slider-pagination-{{ section.id }} {
      gap: 10px;
    }

    .slider-pagination-{{ section.id }} .swiper-pagination-bullet {
      width: {{ dots_size }}px;
      flex: 0 0 {{ dots_size }}px;
      height: {{ dots_size }}px;
    }
  }
  
{%- endstyle -%}

{% unless full_width %}
  <style>
    .section-{{ section.id }}-settings {
      max-width: {{ content_width }}rem;
    }
  </style>
{% endunless %}

{% if heading_custom %}
    <style>
        .slider-heading-{{ section.id }} * {
            font-family: {{ heading_font.family }}, {{ heading_font.fallback_families }};
            font-weight: {{ heading_font.weight }};
            font-style: {{ heading_font.style }};
        }
    </style>
{% endif %}

{% if title_custom %}
    <style>
        .slider-title-{{ section.id }} * {
            font-family: {{ title_font.family }}, {{ title_font.fallback_families }};
            font-weight: {{ title_font.weight }};
            font-style: {{ title_font.style }};
        }
    </style>
{% endif %}

{% if image_ratio_mobile == "portrait" %}
    <style>
        .slider-image-{{ section.id }} {
            aspect-ratio: 9.6/12; 
        }
    </style>
{% elsif image_ratio_mobile == "landscape" %}
    <style>
        .slider-image-{{ section.id }}{
            aspect-ratio: 12/10; 
        }
    </style>
{% elsif image_ratio_mobile == "square" %}
    <style>
        .slider-image-{{ section.id }} {
            aspect-ratio: 12/12; 
        }
    </style>
{% else %}
    <style>
        .slider-image-{{ section.id }} {
            aspect-ratio: auto; 
        }
    </style>
{% endif %}

{% if image_ratio == "portrait" %}
    <style>
        @media(min-width: 1024px) {
            .slider-image-{{ section.id }} {
                aspect-ratio: 9.6/12; 
            } 
        }
    </style>
{% elsif image_ratio == "landscape" %}
    <style>
        @media(min-width: 1024px) {
            .slider-image-{{ section.id }} {
                aspect-ratio: 12/10; 
            }
        }
    </style>
{% elsif image_ratio == "square" %}
    <style>
        @media(min-width: 1024px) {
            .slider-image-{{ section.id }} {
                aspect-ratio: 12/12; 
            }
        }
    </style>
{% else %}
    <style>
        @media(min-width: 1024px) {
            .slider-image-{{ section.id }} {
                aspect-ratio: auto; 
            }
        }
    </style>
{% endif %}

{% if image_shape == "none" %}
    <style>
        .slider-image-{{ section.id }} {
            border-top-left-radius: {{ image_top_left_radius }}px;
            border-top-right-radius: {{ image_top_right_radius }}px;
            border-bottom-left-radius: {{ image_bottom_left_radius }}px;
            border-bottom-right-radius: {{ image_bottom_right_radius }}px;
            border: {{ image_border_thickness }}px solid {{ image_border_color }};
        }
    </style>
{% endif %}

{% if arrow_show_mobile %}
    <style>
        .slider-btn-prev-{{ section.id }},
        .slider-btn-next-{{ section.id }},
        .slider-buttons-{{ section.id }} {
            display: flex;
        }
    </style>
{% else %}
    <style>
        .slider-heading-{{ section.id }} {
            text-align: {{ heading_align_mobile }};
        }

        .slider-btn-prev-{{ section.id }},
        .slider-btn-next-{{ section.id }},
        .slider-buttons-{{ section.id }} {
            display: none;
        }
    </style>
{% endif %}

{% if arrow_show_desktop %}
    <style>
        @media(min-width: 1024px) {
            .slider-btn-prev-{{ section.id }},
            .slider-btn-next-{{ section.id }},
            .slider-buttons-{{ section.id }} {
                display: flex;
            }

            .slider-heading-{{ section.id }} {
                text-align: left;
            }
        }
    </style>
{% else %}
    <style>
        @media(min-width: 1024px) {
            .slider-btn-prev-{{ section.id }},
            .slider-btn-next-{{ section.id }},
            .slider-buttons-{{ section.id }}{
                display: none;
            }

            .slider-heading-{{ section.id }} {
                text-align: {{ heading_align }};
            }
        }
    </style>
{% endif %}

{% if dots_show_mobile %}
    <style>
        .slider-pagination-{{ section.id }} {
            display: flex !important;
        }
    </style>
{% else %}
    <style>
        .slider-pagination-{{ section.id }} {
            display: none !important;
        }
    </style>
{% endif %}

{% if dots_show_desktop %}
    <style>
        @media(min-width: 1024px) {
            .slider-pagination-{{ section.id }} {
                display: flex !important;
            }
        }
    </style>
{% else %}
    <style>
        @media(min-width: 1024px) {
            .slider-pagination-{{ section.id }} {
                display: none !important;
            }
        }
    </style>
{% endif %}

{% if arrow_position == "bottom" %}
    <style>
        .slider-heading-{{ section.id }} {
            text-align: {{ heading_align_mobile }} !important;
        }

        @media(min-width: 1024px) {
            .slider-heading-{{ section.id }} {
                text-align: {{ heading_align }} !important;
            }
        }
    </style>
{% endif %}

{% if margin_horizontal_mobile > 0 %}
  <style>
    .section-{{ section.id }} {
      border-left: solid {{ border_color }} {{ border_thickness }}px;
      border-right: solid {{ border_color }} {{ border_thickness }}px;
    }
    
    @media(min-width: 1024px) {
      .section-{{ section.id }} {
        border-left: 0px;
        border-right: 0px;
      }
    }
  </style>
{% endif %}

{% if margin_horizontal > 0 %}
  <style>
    @media(min-width: 1024px) {
      .section-{{ section.id }} {
        border-left: solid {{ border_color }} {{ border_thickness }}px;
        border-right: solid {{ border_color }} {{ border_thickness }}px;
      }
    }
  </style>
{% endif %}

{% comment %} PREVIEW {% endcomment %}
<style>
  .slider-slider-{{ section.id }}.preview .swiper-wrapper {
    display: flex;
    align-items: center;
    justify-content: space-between;
    overflow: hidden;
  }

  .slider-slider-{{ section.id }}.preview .slider-slide-{{ section.id }} {
    width: 100%;
  }

  .slider-slider-{{ section.id }}.preview .slider-slide-{{ section.id }}:not(:first-child) {
    margin-left: {{ slider_gap_mobile }}px;
  } 

  @media(min-width: 1024px) {
    .slider-slider-{{ section.id }}.preview .slider-slide-{{ section.id }}:not(:first-child) {
      margin-left: {{ slider_gap }}px;
    } 

    .slider-slider-{{ section.id }}.preview .slider-slide-{{ section.id }} {
      width: 18%;
    }
  }
</style>

<div class="section-{{ section.id }} slider-{{ section.id }}" style="background-color:{{ background_color }}; background-image: {{ background_gradient }};">
    <div class="section-{{ section.id }}-settings">
        <div class="slider-top-{{ section.id }}">
            {% if heading != blank %}
                <div class="slider-heading-{{ section.id }}">
                    {{ heading }}
                </div>
            {% endif %}
            {% if arrow_position == "top" %}
                <div class="slider-buttons-{{ section.id }}">
                    <button class="slider-btn-prev-{{ section.id }}">
                        {% if arrow_left != blank %}
                            <img src="{{ arrow_left | image_url }}" alt="{{ arrow_left.alt }}">
                        {% else %}
                            <svg class="theme-icon" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor">
                                <path stroke-linecap="round" stroke-linejoin="round" d="M10.5 19.5L3 12m0 0l7.5-7.5M3 12h18"></path>
                            </svg>
                        {% endif %}
                    </button>
                    <button class="slider-btn-next-{{ section.id }}">
                        {% if arrow_right != blank %}
                            <img src="{{ arrow_right | image_url }}" alt="{{ arrow_right.alt }}">
                        {% else %}
                        <svg class="theme-icon" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor">
                            <path stroke-linecap="round" stroke-linejoin="round" d="M10.5 19.5L3 12m0 0l7.5-7.5M3 12h18"></path>
                        </svg>
                        {% endif %}
                    </button>
                </div>
            {% endif %}
        </div>
    </div>
    <div class="slider-slider-{{ section.id }} swiper preview">
        <div class="swiper-wrapper">
            {% for block in section.blocks %}
                <{% if block.settings.url != blank %}a href="{{ block.settings.url }}"{% else %}div{% endif %} class="slider-slide-{{ section.id }} swiper-slide">
                    <div class="slider-image-{{ section.id }}" style="clip-path: url(#image-shape-{{ section.id }});">
                        {% if block.settings.image != blank %}
                            <img 
                                src="{{ block.settings.image | image_url }}" 
                                alt="{{ block.settings.image.alt }}" 
                                {% if lazy %}
                                loading="lazy"
                                {% endif %}>
                        {% else %}
                            {{ 'image' |  placeholder_svg_tag }}
                        {% endif %}
        
                        {% if image_shape == "1" %}
                            <svg class="clip-{{ section.id }}" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="0px" height="0px" viewBox="0 0 340 341" fill="none">
